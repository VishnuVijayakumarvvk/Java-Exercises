package firstoops;

public class Cars {
	private String car1;
	private String door;
	private int speed;
	
	public Cars() {
		this.car1="Honda";
		this.door="opene";
		this.speed=70;
	}
	
	
	
	public Cars(String car1, String door, int speed) {
		this.car1 = car1;
		this.door = door;
		this.speed = speed;
	}



	public String getCar1() {
		return car1;
	}
	public void setCar1(String car1) {
		this.car1 = car1;
	}
	public String getCar2() {
		return door;
	}
	public void setCar2(String car2) {
		this.door = car2;
	}
	public int getNumber() {
		return speed;
	}
	public void setNumber(int number) {
		this.speed = number;
	}
	
	public String havCar() {
		if(car1.equals("Honda")&& door.equals("opened") && speed >= 10 ) {
			return "Car is running";
		}else {
			return "Car is not running";
		}
	}

}
